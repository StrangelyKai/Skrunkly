{
	_item = _argv[0]
	_first = SUBSTR(_item,0,1)
	_first = TOLOWER(_first)
	
	if _first == "a" || _first == "e" || _first == "i" || _first == "o" || _first == "u"
	{
		"an"
	}
	else
	{
		"a"
	}
}


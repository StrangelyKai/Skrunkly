ext : nonoverlap
{
	"."
	--
	_ext = _argv[0]
	
    _img = ("png","jpg","jpeg","gif")
    _music = ("mp3","mp4","wav")
    _word = ("txt","pdf","docx")

    if _ext == "img"; ANY(_img)
    elseif _ext == "music"; ANY(_music)
    elseif _ext == "word"; ANY(_word)
    else; ANY(_img,_music,_word)
}

